				"orientation":0,
				"l":0.05,
				"t":0.0,
				"r":0.95,
				"b":0.8,
			}
		}

		{
			"name":"apLanding",
			"class":"APcopter_landing",
			"bInst":0,
			"Window":"apView",
			"APcopter_base":"apBase",
			"_Stream":"ZED",
			"_AIbase":"detectNet",
			"_Automaton":"apState",
			"orientationX":1,
			"orientationY":1,
			"timeout":1000,
			"minSize":0.0,
			"maxSize":0.5,
		}
		
	}
