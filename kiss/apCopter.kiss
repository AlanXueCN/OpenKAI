		{
			"name":"apAvoid",
			"class":"APcopter_avoid",
			"bInst":0,
			"Window":"apView",
			"_Automaton":"apState",
			"APcopter_base":"apBase",

			{
				"name":"distSensor",
				"bInst":1,
		
				{
					"name":"Obstacle",
					"class":"_Obstacle",

					{
						"orientation":0,
						"l":0.05,
