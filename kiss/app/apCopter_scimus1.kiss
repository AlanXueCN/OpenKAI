{
	"name":"apPilot",
	"class":"_AutoPilot",
	"_Automaton":"apState",
	"bInst":1,
	"FPS":30,
	"Window":"OKview",
	
	{
		"name":"action",
		
		{
			"name":"apBase",
			"class":"APcopter_base",
			"bInst":1,
			"_Automaton":"apState",
			"Window":"OKview",
			"_Mavlink":"mavAP",
			"freqAtti":0,
			"freqGlobalPos":0,
			"freqHeartbeat":0,
		}
		
		{
			"name":"apThrust",
			"class":"APcopter_thrust",
			"bInst":1,
			"bLog":1,
			"Window":"OKview",
			"_Automaton":"apState",
			"activeState":["CC_ON"],
			"APcopter_base":"apBase",
			"_SlamBase":"lidarSlam",
			"_WebSocket":"apWS",
			"_Mavlink_GCS":"mavGCS",
			"rcTimeOut":1000000,
			"PIDroll":"PIDroll",
			"PIDpitch":"PIDpitch",
			"PIDyaw":"PIDyaw",
			"PIDalt":"PIDalt",
			"pwmLow":1000,
			"pwmMid":1500,
			"pwmHigh":2000,
			"targetMin":1.5,
			"targetMax":15,
			"dCollision":3.0,
		}
		
	}
